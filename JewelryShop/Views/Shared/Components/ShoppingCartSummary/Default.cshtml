@model ShoppingCartViewModel


@if (Model.ShoppingCart.ShoppingCartItems.Count > 0)
{
    <a asp-controller="ShoppingCart" asp-action="Index"><span class="ti-shopping-cart">(@Model.ShoppingCart.ShoppingCartItems.Count)</span></a>

}
else
{
    <a href="#"><span class="ti-shopping-cart"></span></a>

}